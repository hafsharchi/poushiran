    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "poushiran": "file:",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.16"
  },
